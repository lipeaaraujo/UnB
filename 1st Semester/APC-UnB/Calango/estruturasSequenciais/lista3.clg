algoritmo distanciaPontos;
// Síntese
//  Objetivo:  Calcular a distância entre dois pontos.
//  Entrada : Coordenadas dos dois pontos.
//  Saída   : Distância entre os pontos.

principal
	// Declarações

	real x1, y1, x2, y2, distancia;

	// Instruções

	leia(x1);
	leia(y1);
	leia(x2);
	leia(y2);

	distancia = raizQuadrada(exp((x2 - x1), 2)+ exp((y2 - y1), 2));
	
	escreval("DISTANCIA = ", distancia::2);

fimPrincipal
